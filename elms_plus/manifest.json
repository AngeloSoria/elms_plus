{
    "manifest_version": 3,
    "name": "eLMS+",
    "description": "Enhance your eLMS experience!",
    "version": "0.0.1",
    "icons": {"128": "./images/Icon_128.png"},
    "content_scripts": [
        {
            "matches": [
                "https://elms.sti.edu/*"
            ],
            "js": ["Content.js"]
        }
    ],
    "action":{
        "default_popup": "/ui/popup/index.html",
        "default_title" : "Open eLMS+"
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "background":{
        "service_worker": "background.js"
    }
}